package com.eclipse.test;
public class CommandLine {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(i=0 ; i<args.length();i++) {
		
		double factorial=1;
		int i = userinput;
		
		while(i>0) {
			factorial = i*factorial;
			i--;
		}
		
		System.out.println(factorial);
		
		System.out.println(factorial);
	}
	}
}
